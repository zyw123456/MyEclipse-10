#center{max-width:1440px; height: 540px;  margin:0 auto; position:relative;}
.bgLeft{width:58%;height:540px; float:left; display:block; position:absolute; left:0px; top:0px;}
.bgRight{width:37%;height:540px; float:right;display:block; position:absolute; right:0px; top:0px;}
