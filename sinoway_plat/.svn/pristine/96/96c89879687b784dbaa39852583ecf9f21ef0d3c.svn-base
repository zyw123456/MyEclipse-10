.form h1,h2,#upd,#upds{ margin-left:24px;}
.form h1{ margin-top:20px;}
.form h1,h2{ font-weight:inherit; color:#333; line-height:90px;}
.form h2{ font-weight:inherit; color:#333; line-height:40px; color:#666; }
#upd,#upds{  background-color:#FFF; width:78%; border-radius:4px; margin-top:20px; line-height:40px;}
#upd span,#upds span{ display:block; width:12%; margin-top:6px; float:left;  margin-left:6px; background-image:url(../images/shu.png); background-repeat:no-repeat; background-position:right 0px;}
.usertex{ line-height:30px; border-left:#999 1px solid; width:75%; margin-left:6px;}
.nopass{ width:78%; margin-left:24px; position:relative; margin-top:15px;}
.nopass input{ width:auto; height:auto; position:absolute; left:0px; top:2px; display:block; }
.nopass span{ position:absolute; left:13px;}
.nopass span.password{ width:84px; height:16px; position:absolute; right:0px; top:0px; display:block;}
.nopass a{display:inline-block; float:right; text-decoration:none; color:#38a0b9;}

.qingbu{ margin-left:20px; color:#999;}


#round {
	margin:0 auto;
	margin-top:40px;
     width:70%; height:40px;
	background-color:#38a0b9;
    -moz-border-radius: 6px;      /* Gecko browsers */
    -webkit-border-radius: 6px;   /* Webkit browsers */
    border-radius:6px;            /* W3C syntax */
	line-height:40px;
	text-align:center;
	color:#FFF;
	font-size:16px;
}
#round a{text-decoration:none; color:#FFF;}
#round a:hover{  font-size:18px;}
#round:hover{ background-color:#066;;}


