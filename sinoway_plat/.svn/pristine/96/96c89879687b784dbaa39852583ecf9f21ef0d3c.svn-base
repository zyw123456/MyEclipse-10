#mytable2 img{vertical-align:middle; display:inline-block;}
#mytable td{ margin-left:60px; padding:10px 0; padding-left:20px; line-height:26px;}

#mytable2{ margin-left:50px; -webkit-box-shadow:0 2px 16px  #999999;-moz-box-shadow:0 2px 16px #999999;float:left; height: 490px;}
