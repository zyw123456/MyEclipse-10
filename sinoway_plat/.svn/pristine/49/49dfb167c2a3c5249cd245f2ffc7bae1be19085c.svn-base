package com.sinoway.common.entity;


/**
 * 通用的报文响应消息
 * @author miles
 *
 */
public class GeneralRptResMsg {

	public GeneralRptResMsg() {
		super();
	}
	
	public GeneralRptResMsg(GeneralRptMsgHeader header, GeneralRptResMsgBody body) {
		super();
		this.header = header;
		this.body = body;
	}
	/**
	 * 通用报文消息头
	 */
	private GeneralRptMsgHeader header;

	/**
	 * 通用报文消息体
	 */
	private GeneralRptResMsgBody body;

	public GeneralRptMsgHeader getHeader() {
		return header;
	}

	public void setHeader(GeneralRptMsgHeader header) {
		this.header = header;
	}

	public GeneralRptResMsgBody getBody() {
		return body;
	}

	public void setBody(GeneralRptResMsgBody body) {
		this.body = body;
	}
	
	
}
