html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td
	{
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-weight: inherit;
	font-style: inherit;
	font-family: inherit;
	vertical-align: baseline;
	font-size: 12px;
	background-repeat: repeat;
}

body {
	color: #2f332a;
	font: 15px/21px Arial, Helvetica, simsun, sans-serif;
	background: #f0f6e4 \9;
}

h1,h2,h3,h4,h5,h6 {
	color: #2f332a;
	font-weight: bold;
	font-family: Helvetica, Arial, sans-serif;
	padding-bottom: 5px;
}

h1 {
	font-size: 24px;
	line-height: 34px;
	text-align: center;
}

h2 {
	font-size: 14px;
	line-height: 24px;
	padding-top: 5px;
}

h6 {
	font-weight: normal;
	font-size: 12px;
	letter-spacing: 1px;
	line-height: 24px;
	text-align: center;
}

a {
	color: #3C6E31;
	text-decoration: underline;
}

input.radio {
	margin: 0 2px 0 8px;
}

input.radio.first {
	margin-left: 0;
}

input.empty {
	color: lightgray;
}

code {
	color: #2f332a;
}

.highlight_red {
	color: #A60000;
}

.highlight_green {
	color: #A7F43D;
}

li {
	list-style: circle;
	font-size: 12px;
	border-bottom: 1px;
}

li.title {
	list-style: none;
}

ul.ztree {
	float: left;
	margin-top: 20px;
	border: 1px solid #617775;
	width: 370px;
	height: 400px;
	overflow-y: scroll;
}

ul,li,h2 {
	margin: 0;
	padding: 0;
}

ul {
	list-style: none;
}

/**自动任务标题*/
#asTitleDiv {
	width: 100%;
	height: 50px;
	position: relative;
	left: 20px;
}

#asTitleDiv label {
	position: relative;
	top: 20px;
	border-top: 10px;
	font-size: 25px;
	font-weight: bolder;
	size: 25px;
	border-top: 10px;
}

#top {
	width: 100%;
	height: 26px;
	/* 	background-color: #c76c6f; */
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
	background-repeat: repeat-x;
}

#top h2 {
	width: 450px;
	height: 24px;
	float: left;
	font-size: 12px;
	text-align: center;
	line-height: 20px;
	/* 	color: #FFF; */
	font-weight: bold;
	padding-top: 2px;
}

#top .split {
	width: 5px;
	float: left;
	height: 26px;
	/* 	background-color: #FFFFFF; */
}

#topTags {
	height: 24px;
	float: left;
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
	padding-top: 2px;
	padding-left: 10px;
}

#topTags ul li {
	float: left;
	width: 100px;
	height: 21px;
	margin-right: 4px;
	display: block;
	text-align: center;
	cursor: pointer;
	padding-top: 3px;
	color: #15428B;
	font-size: 12px;
}

#main {
	width: 100%;
	height: 500px;
	margin-top: 0;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
}

#main .split {
	width: 5px;
	float: left;
	height: 500px;
	/* 	background-color: #c76c6f; */
	margin-left: 10px;
	margin-top: -40px;
}

#leftMenu {
	width: 450px;
	height: 500px;
	float: left;
}

.content {
	width: 500px;
	height: 450px;
	padding: 5px;
	display: none;
	overflow-y: auto;
	line-height: 30px;
	background-color: #ebebe6;
	margin-top: -20px;
}

#footer {
	width: 100%;
	height: 26px;
	line-height: 20px;
	text-align: center;
	margin-top: 10px;
	margin-right: auto;
	margin-bottom: 0;
	margin-left: auto;
	/* 	background-color: #c76c6f;
	color: #FFF; */
}

.submit_but05 {
	width: 55px;
	height: 24px;
	background: url(<%=path%>/common/images/iocn_14.png) no-repeat;
	border: 0px;
}

#changceManager {
	list-style-type: none;
	display: block;
	overflow: hidden;
	width: 676px;
	margin: 0px;
	padding: 0px;
}

#changceManager li {
	float: left;
	text-align: center;
	margin-right: 20px;
}

#changceManager li a {
	color: #7D3333;
	text-decoration: none;
	cursor: pointer;
}

.cont_table {
	border-collapse: collapse;
	margin-top: 1px;
}

.cont_table th {
	border: 1px solid #d8e8f5;
	color: #417395;
	background-color: #eef3ff;
	font-weight: normal;
	line-height: 25px;
}

.cont_table td {
	border: 1px solid #d8e8f5;
	line-height: 25px;
	text-align: center;
	color: #5b5b5b;
}

.cont_table_tr {
	background-color: #e8f2f6;
}

.cont_mar {
	margin-top: 8px;
	margin-right: 12px;
	margin-bottom: 8px;
	margin-left: 12px;
}

.dl_bg {
	color: 5b5b5b;
	margin-left: 20px;
	line-height: 25px;
}

.cont_mar a {
	color: #417395;
	text-decoration: underline;
}

.cont_mar a:hover {
	color: #b10000;
}

.cont_tables {
	border-top: 1px solid #d8e8f5;
	border-left: 1px solid #d8e8f5;
	line-height: 25px;
	color: #5b5b5b;
}

.cont_tables .td_bg {
	border-bottom: 1px solid #d8e8f5;
	border-right: 1px solid #d8e8f5;
	background-color: #eaf3fa;
	text-align: right;
	padding: 5px 5px 5px 0;
}

.cont_tables .td_bk {
	border-bottom: 1px solid #d8e8f5;
	border-right: 1px solid #d8e8f5;
	background-color: #eaf3fa;
	text-align: left;
	padding-top: 5px;
	padding-right: 2px;
	padding-bottom: 5px;
	padding-left: 0;
}

.cont_tables .td_con {
	border-bottom: 1px solid #d8e8f5;
	border-right: 1px solid #d8e8f5;
	padding-top: 5px;
	padding-right: 15px;
	padding-bottom: 5px;
	padding-left: 10px;
}