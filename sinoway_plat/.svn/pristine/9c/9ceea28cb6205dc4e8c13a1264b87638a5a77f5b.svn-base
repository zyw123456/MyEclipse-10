table.dataTable thead th,
table.dataTable thead td{
    padding-left: 10px;
    padding-right: 5px;
}
table.dataTable thead th input,
table.dataTable thead td input{
    float: left;
    margin-top: 3px;
    margin-right: 8px;
    /*margin-bottom: ;*/
}
table.dataTable thead th img,
table.dataTable thead td img{
    float: left;
    margin-right: 8px;
}
table.dataTable thead th a,
table.dataTable thead td a{
    float: left;
    /*font-size: 14px;*/
    color: #333333;
}
table.sorting_1{
    width:10px;
    padding: 0;
}
table.dataTable tbody td p{
    margin: 0;
    cursor: pointer;
    max-width: 110px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
table.dataTable tbody td p.p-all{
    max-width: 100%;
}